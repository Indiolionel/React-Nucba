generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  firstname String
  orders    Order[]
}

model Order {
  id     Int    @id @default(autoincrement())
  name   String
  price  Int
  user   User   @relation(fields: [userId], references: [id])
  userId Int
}
